    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.4.0",
    "typescript": "~4.5.5"
